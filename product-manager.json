      "parameters": {
        "jsCode": "const inputData = $input.first().json;\n\n// Case 1: WhatsApp (Existing Trigger)\nif (inputData.contacts && inputData.messages) {\n  return {\n    platform: 'whatsapp',\n    senderId: inputData.contacts[0].wa_id,\n    senderName: inputData.contacts[0].profile.name || 'User',\n    message: inputData.messages[0].text.body\n  };\n}\n\n// Case 2: Instagram\nif (inputData.object === 'instagram' && inputData.entry) {\n  const msg = inputData.entry[0].messaging[0];\n  return {\n    platform: 'instagram',\n    senderId: msg.sender.id,\n    senderName: 'Instagram User',\n    message: msg.message.text\n  };\n}\n\n// Case 3: Facebook\nif (inputData.object === 'page' && inputData.entry) {\n  const msg = inputData.entry[0].messaging[0];\n  return {\n    platform: 'facebook',\n    senderId: msg.sender.id,\n    senderName: 'Facebook User',\n    message: msg.message.text\n  };\n}\n\nreturn { platform: 'unknown', senderId: 'unknown', message: '' };"
      },
